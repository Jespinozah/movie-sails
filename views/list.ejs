<h3>Movies</h1>
<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% movies.forEach(function(movie){%>
      <tr>
        <td><%= movie.id %></td>
        <td><%= movie.title %></td>
        <td></td>
      </tr>
    <%})%>
  </tbody>
</table>
